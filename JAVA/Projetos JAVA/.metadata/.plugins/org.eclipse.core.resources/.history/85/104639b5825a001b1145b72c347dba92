import java.util.Scanner;

public class Main {

	public static void main(String[]arg) {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Informe um número inteiro: ");
		int x = sc.nextInt();
		
		System.out.println("Os números impares são: ");
		for (int i = 0; i <= x; i++) {
			if (i % 2 != 0) {
				System.out.println(i);
			}
		}
		
		sc.close();
	}
	
}
