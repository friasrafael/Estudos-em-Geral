
public class Main {
	
	public static void main(String[]arg) {
		
		double x = 9.0;
		double y = 5.6;
		double z = -5.0;
		double A, B, C;
		
		A = Math.sqrt(x);
		B = Math.pow(x, y);
		C = Math.abs(z);
		
		System.out.println("A raiz quadrada de "+ x +" é " + A);
		System.out.println(x + " elevado a " + y + " é igual a " + B);
		System.out.printf("%.2f elevado a %.2f é igual a %.2f %n", x, y, B);
		System.out.printf("O valor de Y é %.2f", y);
		System.out.printf("O valor absoluto de %.2f é %.2f", z, C);
		
		
	}
}
