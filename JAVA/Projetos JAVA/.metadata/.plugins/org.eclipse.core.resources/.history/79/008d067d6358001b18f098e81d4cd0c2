import java.util.Scanner;

public class Main {

		public static void main(String[]arg) {
			
			Scanner sc = new Scanner(System.in);
			int n1;
			int n2;
			
			System.out.println("Digite um número inteiro:");
			n1 = sc.nextInt();
			
			System.out.println("Digite outro número inteiro:");
			n2 = sc.nextInt();
			
			sc.close();
		}
}
