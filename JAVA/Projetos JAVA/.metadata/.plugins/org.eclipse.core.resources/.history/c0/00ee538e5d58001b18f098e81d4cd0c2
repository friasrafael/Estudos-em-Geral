import java.util.Scanner;

public class Main {
	
	public static void main(String[]arg) {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Que horas são?");
		int hora = sc.nextInt();
		System.out.printf("Agora são %d horas%n", hora);
		
		if (hora <= 12) {
			System.out.println("Bom dia");
		}
	}
}
