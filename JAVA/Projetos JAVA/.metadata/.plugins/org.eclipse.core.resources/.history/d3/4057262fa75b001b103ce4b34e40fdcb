import java.util.Scanner;

public class Main {

	public static void main(String[] arg) {

		Scanner sc = new Scanner(System.in);
		
		System.out.println("Quantos fatoriais você quer calcular? ");
		int N = sc.nextInt();
		int n1 = 0;
		int fatorial = 1;
		for (int x = 1; x <= N; x++) {
			System.out.printf("Digite o valor a ser calculado:");
			n1= sc.nextInt();
			for (int n2 = n1 - 1; n2 != 0; n2--) {
				fatorial *= (n1 - n2);
			}
		}
		
		sc.close();
	}
}
